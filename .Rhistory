install.packages("fractaldim")
# fractals
library(fractaldim)
signal <- cumsum(rnorm(1000))
# 2. Estimate the dimension using multiple methods
# It is best practice to compare a few methods to ensure consistency
estimates <- fd.estimate(signal, methods = c("boxcount", "variogram", "hallwood"))
# 3. Access the numerical results
cat("Estimated D (Box-count):", estimates$fd$boxcount)
cat("Estimated D (Variogram):", estimates$fd$variogram)
View(estimates)
estimates[["methods"]]
# 2. Estimate the dimension using multiple methods
# It is best practice to compare a few methods to ensure consistency
estimates <- fd.estimate(signal, methods "boxcount")
# 2. Estimate the dimension using multiple methods
# It is best practice to compare a few methods to ensure consistency
estimates <- fd.estimate(signal, methods ="boxcount")
# 3. Access the numerical results
cat("Estimated D (Box-count):", estimates$fd$boxcount)
View(estimates)
# 3. Access the numerical results
cat("Estimated D (Box-count):", estimates$fd)
estimates.v <- fd.estimate(signal, methods ="variogram")
cat("Estimated D (Variogram):", estimates.v$fd)
